{
	"name": "mui-image",
	"version": "1.1.0-beta.0",
	"description": "Display images as per the Material guidelines. For React apps using Material-UI v5.",
	"main": "lib/index.js",
	"module": "es/index.js",
	"files": [
		"es",
		"lib",
		"umd"
	],
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"build:demo": "vite --config vite.democonfig.ts build",
		"preview": "vite preview",
		"size": "size-limit",
		"clean": "rimraf dist es lib umd coverage jest-setup.d.ts",
		"prepare": "npm run build",
		"postversion": "git push && git push --tags",
		"test": "vitest",
		"test:once": "vitest run",
		"test:coverage": "vitest run --coverage"
	},
	"peerDependencies": {
		"@emotion/react": "^11.4.1",
		"@emotion/styled": "^11.3.0",
		"@mui/material": "^5.0.1",
		"react": "^17.0.2 || ^18.0.0"
	},
	"devDependencies": {
		"@fontsource/fira-code": "^4.5.12",
		"@size-limit/preset-small-lib": "^7.0.8",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.4.0",
		"@types/react": "^18.0.26",
		"@types/react-dom": "^18.0.10",
		"@types/react-test-renderer": "^18.0.0",
		"@vitejs/plugin-react": "^3.0.0",
		"@vitest/coverage-c8": "^0.26.3",
		"jsdom": "^20.0.3",
		"rimraf": "^3.0.2",
		"size-limit": "^7.0.8",
		"typeit-react": "^2.6.4",
		"typescript": "^4.9.4",
		"vite": "^4.0.4",
		"vite-plugin-build": "^0.7.0",
		"vitest": "^0.26.3"
	},
	"size-limit": [
		{
			"path": "es/index.js"
		}
	],
	"author": "Miracutor",
	"homepage": "https://miracutor.github.io/mui-image",
	"license": "ISC",
	"repository": "https://github.com/Miracutor/mui-image",
	"keywords": [
		"material-ui",
		"material-ui image",
		"material-ui-image",
		"material design",
		"react",
		"loading image"
	],
	"dependencies": {
		"clsx": "^1.2.1"
	}
}
