{
	"name": "mui-image-alter",
	"version": "3.1.0",
	"description": "Fork of mui-image. Display images as per the Material guidelines using Material-UI v5. With component prop support.",
	"type": "module",
	"main": "./dist/index.cjs",
	"types": "./dist/index.d.cts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"build:demo": "vite --config vite.democonfig.ts build",
		"preview": "vite preview",
		"size": "size-limit",
		"format": "prettier {demo,src}/**/*.{js,jsx,ts,tsx,json} --write",
		"clean": "rimraf dist es lib umd coverage jest-setup.d.ts mui-image-alter-*",
		"prepare": "npm run build",
		"preversion": "npm run clean && npm run format",
		"postversion": "git push && git push --tags",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"test:coverage-ci": "vitest run --coverage.enabled --coverage.reporter='text-summary'"
	},
	"dependencies": {
		"clsx": "^2.1.1",
		"use-resize-observer": "^9.1.0"
	},
	"peerDependencies": {
		"@mui/material": "^5.0.0 || ^6.0.0",
		"react": "^17.0.0 || ^18.0.0",
		"react-dom": "^17.0.0 || ^18.0.0"
	},
	"devDependencies": {
		"@emotion/react": "^11.13.3",
		"@emotion/styled": "^11.13.0",
		"@fontsource/fira-code": "^5.0.19",
		"@size-limit/preset-small-lib": "^11.1.4",
		"@testing-library/jest-dom": "^6.5.0",
		"@testing-library/react": "^16.0.1",
		"@types/react": "^18.3.5",
		"@types/react-dom": "^18.3.0",
		"@vitejs/plugin-react": "^4.3.1",
		"@vitest/coverage-v8": "^2.0.5",
		"jsdom": "^25.0.0",
		"prettier": "^3.3.3",
		"rimraf": "^6.0.1",
		"size-limit": "^11.1.4",
		"typeit-react": "^2.7.2",
		"typescript": "^5.5.4",
		"vite": "^5.4.2",
		"vite-plugin-dts": "^4.1.0",
		"vitest": "^2.0.5"
	},
	"size-limit": [
		{
			"name": "cjs",
			"path": "dist/index.cjs",
			"gzip": true
		},
		{
			"name": "esm",
			"path": "dist/index.js",
			"gzip": true
		}
	],
	"author": "mddanish00",
	"contributors": [
		"benmneb"
	],
	"homepage": "https://mddanish00.github.io/mui-image-alter",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mddanish00/mui-image-alter.git"
	},
	"keywords": [
		"material-ui",
		"material-ui image",
		"material-ui-image",
		"material design",
		"react",
		"loading image"
	]
}