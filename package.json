{
	"name": "mui-image",
	"version": "1.0.7",
	"description": "Display images as per the Material guidelines. For React apps using Material-UI v5.",
	"main": "lib/index.js",
	"module": "es/index.js",
	"files": [
		"es",
		"lib",
		"umd"
	],
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"build:demo": "vite --config vite.democonfig.ts build",
		"preview": "vite preview",
		"size": "size-limit",
		"clean": "rimraf dist es lib umd coverage jest-setup.d.ts",
		"prepare": "npm run build",
		"postversion": "git push && git push --tags",
		"test": "vitest",
		"test:once": "vitest run",
		"test:coverage": "vitest run --coverage",
		"test:coverage-ci": "vitest run --coverage.enabled --coverage.reporter='text-summary'"
	},
	"peerDependencies": {
		"@emotion/react": "^11.4.1",
		"@emotion/styled": "^11.3.0",
		"@mui/material": "^5.0.1",
		"react": "^17.0.2 || ^18.0.0"
	},
	"devDependencies": {
		"@fontsource/fira-code": "^5.0.12",
		"@size-limit/preset-small-lib": "^9.0.0",
		"@testing-library/jest-dom": "^6.1.3",
		"@testing-library/react": "^14.0.0",
		"@types/react": "^18.2.25",
		"@types/react-dom": "^18.2.10",
		"@vitejs/plugin-react": "^4.1.0",
		"@vitest/coverage-v8": "^0.34.6",
		"jsdom": "^22.1.0",
		"rimraf": "^5.0.5",
		"size-limit": "^9.0.0",
		"typeit-react": "^2.6.4",
		"typescript": "^5.2.2",
		"vite": "^4.4.11",
		"vite-plugin-build": "^0.10.0",
		"vitest": "^0.34.6"
	},
	"size-limit": [
		{
			"path": "es/index.js"
		}
	],
	"author": "mddanish00",
	"contributors": [
		"benmneb"
	],
	"homepage": "https://mddanish00.github.io/mui-image",
	"license": "ISC",
	"repository": "https://github.com/mddanish00/mui-image",
	"keywords": [
		"material-ui",
		"material-ui image",
		"material-ui-image",
		"material design",
		"react",
		"loading image"
	],
	"dependencies": {
		"clsx": "^2.0.0"
	}
}